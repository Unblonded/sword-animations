	"authors": [
		"Unblonded"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/sword-blocking/icon.png",
	"environment": "client",
	"entrypoints": {
		"client": [
			"unblonded.swords.SwordBlocking",
			"unblonded.swords.ConfigCommand"
		]
	},
	"mixins": [
		"sword-blocking.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.14",
		"minecraft": "~1.21.5",
		"java": ">=21",
		"fabric-api": "*"
